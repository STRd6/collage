- Tool = require "../presenters/tool"
- editor = this

app
  tools
    - @tools.forEach (tool) ->
      = Tool tool, editor
  workspace
    scene(drop=@workspaceDrop dragover=@workspaceDragover @mousedown @mousemove @mouseup)
    = @screenElement
  tray
    materials(dragstart=@materialDrag)
      = @images
    actions
      button(click=@render) Render
